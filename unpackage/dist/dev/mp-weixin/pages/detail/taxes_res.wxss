.circle_warp{
		width: 100%;
		text-align: center;
		padding-top: 63rpx;
		margin: 0 auto;
		height:480rpx;
}
.circle{
		width:360rpx;
		height:360rpx;
		border:60rpx solid rgba(77,166,255,1);
		border-radius:50%;
		margin: 0 auto;
}
.circle view:first-child{
		margin-top: 149rpx;
		font-size:32rpx;
		font-family:Arial;
		font-weight:bold;
}
.circle view:last-child{
		margin-top: 31rpx;
		color: #999;
		font-size:24rpx;
		font-family:PingFang SC;
		font-weight:500;
}
.desc_warp{
	margin-top: 77rpx;
}
.desc{
	width:100%;
	height:27rpx;
	font-size:28rpx;
	font-family:PingFang SC;
	font-weight:500;
	color:#333333;
	line-height:27rpx;
	margin-bottom: 30rpx;
	text-align: center;
}
.desc text{
	color: #999;
}

